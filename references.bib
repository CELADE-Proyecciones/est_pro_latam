@article{bohk-ewald2018,
  author = {Bohk-Ewald, Christina and Li, Peipei and Myrskylä, Mikko},
  title = {Forecast accuracy hardly improves with method complexity when completing fertility},
  journal = {Proceedings of the National Academy of Sciences},
  volume = {115},
  number = {37},
  pages = {9187--9192},
  year = {2018},
  abstract = {Forecasts of completed fertility predict how many children will be born on average by women over their entire reproductive lifetime. These forecasts are important for policymakers and demographers, but little is known about their accuracy. We compare the accuracy of 13 methods for forecasting completed fertility, ranging from simple averages to complex Bayesian hierarchical models. We use data from 1950 to 2015 for 20 European countries to estimate the methods and evaluate their accuracy in out-of-sample forecasts. We find that forecast accuracy hardly improves with method complexity. The most accurate forecasts are obtained using simple averages of recent observations. Complex models are prone to overfitting and do not outperform simple methods. Our findings suggest that demographers and policymakers should be cautious about using complex models for forecasting completed fertility and that simple methods may be preferable.},
  url = {http://zotero.org/users/2112629/items/KTJ2RZRI}
}

