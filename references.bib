@article{rizzi2016,
  author = {Rizzi, Silvia and others},
  title = {Estimating and projecting trends in death rates using smoothed MCMC},
  journal = {Statistical Methods in Medical Research},
  volume = {25},
  number = {4},
  pages = {1447--1468},
  year = {2016},
  abstract = {Histograms are a common tool to estimate densities non-parametrically. They are extensively encountered in health sciences to summarize data and to visualize the distribution of variables. However, histograms are not always the best choice, especially when the variable of interest is continuous. In this paper, we propose a new method to construct histograms that are smooth, unbiased, and have a reduced variance. The method is based on a Bayesian approach and uses P-splines to model the density. We show that our smooth histograms outperform classical histograms in terms of mean squared error and visual appearance. The method is illustrated with examples from the health sciences.},
  url = {http://zotero.org/users/2112629/items/PZDU7FKF}
}

